{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "order.schema.json",
    "title": "POC order",
    "description": "Schema for POC order data",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "orderDate": {
            "title": "Order Date",
            "type": "string"
        },
        "sellerId": {
            "title": "Seller ID",
            "type": "string"
        },
        "buyerId": {
            "title": "Buyer ID",
            "type": "string"
        },
        "productId": {
            "title": "Product ID",
            "type": "string"
        },
        "price": {
            "title": "Price",
            "type": "number"
        },
        "quantity": {
            "title": "Quantity",
            "type": "number"
        },
        "requiredDate": {
            "title": "Required Date",
            "type": "string"
        }
    },
    "required": [
        "orderDate",
        "sellerId",
        "buyerId",
        "productId",
        "price",
        "quantity",
        "requiredDate"
    ],
    "additionalProperties": false
}